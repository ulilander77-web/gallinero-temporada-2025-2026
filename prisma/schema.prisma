generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Gallina {
  id          Int      @id @default(autoincrement())
  nombre      String
  nacimiento  DateTime
  raza        String
  cantidadHuevos Int    @default(0)
}
